{
  "manifest_version": 3,
  "name": "Go",
  "version": "1.3",
  "description": "Custom shortcut launcher from the address bar.",
  "permissions": ["tabs", "storage", "omnibox"],
  "background": {
    "service_worker": "background.js"
  },
  "omnibox": {
    "keyword": "go"
  },
  "action": {
    "default_popup": "popup.html",
    "default_title": "Manage Shortcuts",
    "default_icon": {
      "128": "icon.png"
    }
  }
}
