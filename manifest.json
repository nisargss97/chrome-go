{
    "manifest_version": 3,
    "name": "Go",
    "version": "1.1",
    "description": "Custom shortcut launcher from the omnibox.",
    "permissions": ["tabs", "storage"],
    "background": {
        "service_worker": "background.js"
    },
    "omnibox": {
        "keyword": "go"
    },
    "action": {
        "default_popup": "popup.html",
        "default_title": "Manage Shortcuts"
    }
}
  